[run]
source =
    backend/accounts
    backend/predictions
omit =
    */migrations/*
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    manage.py
    */wsgi.py
    */asgi.py
    */settings.py

[report]
skip_empty = True
show_missing = True
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == "__main__":
    @abstractmethod
    @abc.abstractmethod
    if TYPE_CHECKING:
    ...
